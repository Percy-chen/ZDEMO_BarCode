<mvc:View controllerName="ZDEMO_BarCode.ZDEMO_BarCode.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Input id="Zsptm"/>
						<Button text="Scan" press="onPress" icon="sap-icon://bar-code"/>
						<Button text="Camera" press="handleCamera" icon="sap-icon://bar-code"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>